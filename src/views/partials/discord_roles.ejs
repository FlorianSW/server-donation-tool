<% if (step === 'PACKAGE_SELECTION' && user.discordRoles.length !== 0) { %>
    <div class="row">
        <div class="col s12">
            <ul class="collection with-header">
                <li class="collection-header"><h5><%= translate('ASSIGNED_DISCORD_ROLE') %></h5></li>
                <% user.discordRoles.forEach(function (r) { %>
                    <li class="collection-item"><%= r %></li>
                <% }); %>
            </ul>
        </div>
    </div>
<% } %>
