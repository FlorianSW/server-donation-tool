<% if (step === 'PACKAGE_SELECTION' && serversWithPrio.length !== 0) { %>
    <div class="row">
        <div class="col s12">
            <ul class="collection with-header">
                <li class="collection-header"><h5><%= translate('SERVERS_WITH_PRIORITY_QUEUE') %></h5></li>
                <% serversWithPrio.forEach(function (prio) { %>
                    <li class="collection-item"><%= nameFromServerApiId(prio[0]) %> (<%= translate('SERVERS_WITH_PRIORITY_QUEUE_UNTIL') %> <%= prio[1].expires.toLocaleString() %>)</li>
                <% }); %>
            </ul>
        </div>
    </div>
<% } %>
