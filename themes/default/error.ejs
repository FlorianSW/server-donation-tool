<%- include('partials/header.ejs') %>
<div class="container">
    <div class="row">
        <div class="col s10 offset-s1">
            <div class="card">
                <div class="card-image">
                    <i class="material-icons error">highlight_off</i>
                </div>
                <div class="card-content">
                    <span class="card-title">
                        <%= status %>: <%= translate('ERROR_GENERIC_TITLE') %>
                    </span>
                    <p>
                        <%- translate('ERROR_GENERIC_DESCRIPTION') %>
                    </p>
                    <h5><%- translate('ERROR_GENERIC_SUPPORT') %></h5>
                    <code class="support">
                        <%= supportInfo %>
                    </code>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('partials/footer.ejs') %>
